                            <a href="#" class="c-sidebar-nav-item__container">
                                <span class="c-sidebar-nav-item__label">Equipes</span>
                            </a>
                        </li>
                    </ul>
                </li>
            </ul>
        </div>
    </div>
    <div class="o-page-content">
        <div class="o-container">
            {% block body %}{% endblock %}
        </div>
    </div>
</div>

{% block footscripts %}
    {{ encore_entry_script_tags('app') }}
{% endblock %}
</body>
</html>
